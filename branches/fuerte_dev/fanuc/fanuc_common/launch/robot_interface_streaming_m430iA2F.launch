<!--
  Manipulator specific version of 'robot_interface_streaming.launch'.

  Defaults supplied for M-430iA2/F:
   - J23_factor = -1
   - use_bswap = true
   - 5 joints

  Usage:
    robot_interface_streaming_m430iA2F.launch robot_ip:=<value>
-->
<launch>
  <!-- robot_ip: IP-address of the robot's socket-messaging server -->
  <arg name="robot_ip" />

  <arg name="J23_factor" default="-1" />
  <arg name="use_bswap" default="true" />
  <rosparam param="controller_joint_names">[joint_1, joint_2, joint_3, joint_4, joint_5]</rosparam>

  <include file="$(find fanuc_common)/launch/robot_interface_streaming.launch">
    <arg name="robot_ip"   value="$(arg robot_ip)" />
    <arg name="J23_factor" value="$(arg J23_factor)" />
    <arg name="use_bswap"  value="$(arg use_bswap)" />
  </include>
</launch>

