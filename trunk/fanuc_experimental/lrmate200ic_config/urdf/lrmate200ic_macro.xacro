<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
	<xacro:macro name="lrmate200ic" params="prefix">


		<!-- links -->
		<link name="${prefix}base_link">
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://lrmate200ic_config/meshes/lrmate200ic/visual/base_link.stl"/>
				</geometry>
				<material name="fblack">
					<color rgba="0.15 0.15 0.15 1" />
				</material>
			</visual>
			<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://lrmate200ic_config/meshes/lrmate200ic/collision/base_link.stl"/>
				</geometry>
			</collision>
		</link>
		<link name="${prefix}link_1">
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://lrmate200ic_config/meshes/lrmate200ic/visual/link_1.stl"/>
				</geometry>
				<material name="fyel">
					<color rgba="0.9 0.9 0 1" />
				</material>
			</visual>
			<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://lrmate200ic_config/meshes/lrmate200ic/collision/link_1.stl"/>
				</geometry>
			</collision>
		</link>
		<link name="${prefix}link_2">
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://lrmate200ic_config/meshes/lrmate200ic/visual/link_2.stl"/>
				</geometry>
				<material name="fyel" />
			</visual>
			<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://lrmate200ic_config/meshes/lrmate200ic/collision/link_2.stl"/>
				</geometry>
			</collision>
		</link>
		<link name="${prefix}link_3">
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://lrmate200ic_config/meshes/lrmate200ic/visual/link_3.stl"/>
				</geometry>
				<material name="fyel" />
			</visual>
			<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://lrmate200ic_config/meshes/lrmate200ic/collision/link_3.stl"/>
				</geometry>
			</collision>
		</link>
		<link name="${prefix}link_4">
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://lrmate200ic_config/meshes/lrmate200ic/visual/link_4.stl"/>
				</geometry>
				<material name="fyel" />
			</visual>
			<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://lrmate200ic_config/meshes/lrmate200ic/collision/link_4.stl"/>
				</geometry>
			</collision>
		</link>
		<link name="${prefix}link_5">
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://lrmate200ic_config/meshes/lrmate200ic/visual/link_5.stl"/>
				</geometry>
				<material name="fyel" />
			</visual>
			<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://lrmate200ic_config/meshes/lrmate200ic/collision/link_5.stl"/>
				</geometry>
			</collision>
		</link>
		<link name="${prefix}link_6">
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://lrmate200ic_config/meshes/lrmate200ic/visual/link_6.stl"/>
				</geometry>
				<material name="fblack" />
			</visual>
			<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://lrmate200ic_config/meshes/lrmate200ic/collision/link_6.stl"/>
				</geometry>
			</collision>
		</link>
		<link name="${prefix}tool0" />


		<!-- joints -->
		<joint name="${prefix}joint_1" type="revolute">
			<origin xyz="0 0 0.33" rpy="0 0 0" />
			<parent link="${prefix}base_link" />
			<child link="${prefix}link_1" />
			<axis xyz="0 0 1" />
			<limit lower="-2.965" upper="2.965" effort="0" velocity="6.11" />
		</joint>
		<joint name="${prefix}joint_2" type="revolute">
			<origin xyz="0.075 0 0" rpy="0 0 0" />
			<parent link="${prefix}link_1" />
			<child link="${prefix}link_2" />
			<axis xyz="0 1 0" />
			<limit lower="-1.745" upper="1.745" effort="0" velocity="6.11" />
		</joint>
		<joint name="${prefix}joint_3" type="revolute">
			<origin xyz="0 0 0.3" rpy="0 0 0" />
			<parent link="${prefix}link_2" />
			<child link="${prefix}link_3" />
			<axis xyz="0 -1 0" />
			<limit lower="-3.385" upper="3.385" effort="0" velocity="6.98" />
		</joint>
		<joint name="${prefix}joint_4" type="revolute">
			<origin xyz="0.32 0 0.075" rpy="0 0 0"/>
			<parent link="${prefix}link_3"/>
			<child link="${prefix}link_4"/>
			<axis xyz="-1 0 0"/>
			<limit lower="-3.315" upper="3.315" effort="0" velocity="7.85"/>
		</joint>
		<joint name="${prefix}joint_5" type="revolute">
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<parent link="${prefix}link_4"/>
			<child link="${prefix}link_5"/>
			<axis xyz="0 -1 0"/>
			<limit lower="-2.095" upper="2.095" effort="0" velocity="7.85" />
		</joint>
		<joint name="${prefix}joint_6" type="revolute">
			<origin xyz="0.08 0 0" rpy="3.1416 -1.5708 0"/>
			<parent link="${prefix}link_5"/>
			<child link="${prefix}link_6"/>
			<axis xyz="0 0 -1"/>
			<limit lower="-6.285" upper="6.285" effort="0" velocity="12.57" />
		</joint>
		<joint name="${prefix}joint_6-tool0" type="fixed">
			<parent link="${prefix}link_6" />
			<child link="${prefix}tool0" />
			<origin xyz="0 0 0" rpy="0 0 0" />
		</joint>

	</xacro:macro>
</robot>
